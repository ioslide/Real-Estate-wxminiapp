<view class="detail-container data-v-cfeaf816">
  <view class="broker-info-module data-v-cfeaf816">
    <view class="broker-card data-v-cfeaf816">
      <view class="broker-header-box data-v-cfeaf816">
        <view class="portrait-box data-v-cfeaf816">
          <image class="portrait data-v-cfeaf816" mode="aspectFit" src="{{detailDailiren.zhaopian}}"></image>
          <view class="mark data-v-cfeaf816">{{detailDailiren.dengji}} </view>
        </view>
        <view class="base-info-box data-v-cfeaf816">
          <view class="text-box data-v-cfeaf816">
            <view class="name data-v-cfeaf816">{{detailDailiren.mingcheng}} </view>
              <image class="icon data-v-cfeaf816" wx:if="{{detailDailiren.dailidengji == '金牌'}}" mode="aspectFill" src="https://fhy-file.bgy.com.cn/bu00310-prd1/5ed9c685e4b04e6375de9925.png"
                role="img"></image>
              <image class="icon data-v-cfeaf816" wx:if="{{detailDailiren.dailidengji == '银牌'}}" mode="aspectFill" src="https://fhy-file.bgy.com.cn/bu00310-prd1/5ed9c685e4b04e6375de9939.png"
                role="img"></image>
              <image class="icon data-v-cfeaf816" wx:if="{{detailDailiren.dailidengji == '铜牌'}}"  mode="aspectFill" src="https://fhy-file.bgy.com.cn/bu00310-prd1/5ed9c685e4b04e6375de993a.png"
                role="img"></image>
          </view>

          <view class="info data-v-cfeaf816">服务年限：{{detailDailiren.workYear}}年</view>
          <view class="broker-credentials data-v-cfeaf816">
            <view class="items data-v-cfeaf816">
              <image class="icon data-v-cfeaf816" mode="aspectFit"
                src="https://i.qfangimg.com/resource/qfang-mobile-wx/img/icon-broker-1.png?20201228"></image>
              <view class="text data-v-cfeaf816">经纪人证</view>
            </view>
            <view class="items data-v-cfeaf816">
              <image class="icon data-v-cfeaf816" mode="aspectFit"
                src="https://i.qfangimg.com/resource/qfang-mobile-wx/img/icon-broker-2.png?20201228"></image>
              <view class="text data-v-cfeaf816">发房机构</view>
            </view>
          </view>
        </view>
      </view>
      <view class="broker-tag-box data-v-cfeaf816">
        <view class="tag data-v-cfeaf816" wx:for="{{detailDailiren.renwubiaoqian}}" wx:key="index">{{item}}</view>
      </view>
      <view class="broker-attr-box data-v-cfeaf816">
        <view class="items data-v-cfeaf816">
          <view class="label data-v-cfeaf816">主营区域</view>
          <view class="showServiceArea">{{detailDailiren.chengshi}}</view>
          <view class="icon data-v-cfeaf816">
          </view>
        </view>
        <view class="items data-v-cfeaf816" bind:tap="handlePhone" data-phone="{{detailDailiren.dianhua}}">
          <view class="label data-v-cfeaf816">联系电话</view>
          <view class="showServiceArea"> {{detailDailiren.dianhua}}</view>
          <view class="icon data-v-cfeaf816">
          </view>
        </view>
      </view>
    </view>
  </view>
</view>



<view class="common-title padding-left-20 padding-right-20" style="margin-top:0;margin-bottom:0">
  <view class="cuIcon-titles common-title-left" style="font-size: 16px;">代理楼盘</view>
</view>

<view class="cu-card article padding-bottom-30" id="loupandaquan">
  <view class="cu-item shadow margin-left-15 margin-right-15 margin-top-5 " style="" wx:for="{{dailirenHouseGroup}}"
    wx:key="_id" bindtap="navHousedetail" data-id="{{item._id}}">
    <view class="content" style="padding:0px">
      <image src="{{item.loupanxiangce[0]}}" mode="aspectFill"></image>
      <view class="desc">
        <view class="margin-bottom-5" style="font-weight:600;">{{item.loupanmingcheng}}</view>
        <view class="margin-bottom-5" style="color: #999;">{{item.quxian}} | 建面：{{item.jianzumianji}}</view>
        <view class="margin-bottom-5" style="color: #ff6d6d">{{item.loupandanjia}}</view>
        <view>
          <view class="cu-tag bg-gold font-weight-400 text-white sm"
            style="border-radius:0px;margin:5px 5px 0 0;height:20px;font-size:12px;line-height: 20px;color:#fff">
            {{item.chanpinleixing}}
          </view>
          <view wx:for="{{item.biaoqian}}" wx:key="_id" class="cu-tag bg-grey light sm"
            style="border-radius:0px;margin:5px 5px 0 0;height:20px;font-size:12px;line-height: 20px;">{{item}}
          </view>
        </view>
      </view>
    </view>
  </view>
</view>


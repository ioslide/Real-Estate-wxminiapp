<!-- <view class="apartment-box border-box" wx:if="{{apartment.length>0}}"> -->
<view class="apartment-box border-box">
  <view class="common-title ">
    <view class="common-title-left">户型展示</view>
  </view>
  <scroll-view class="apartment-scroll-box border-box" scrollX="true">
    <view bind:tap="goDetail" class="apartment-item border-box" wx:for="{{huxing}}" wx:key="_id">
      <view class="apartment-img-box">
        <image binderror="imgError" class="apartment-img border-box" data-index="{{index}}" lazyLoad="true"
          mode="aspectFit" src="{{item.huxingtupian}}"></image>
      </view>
      <view class="apartment-top border-box">
        <view class="apartment-top-font border-box textOverflow">{{item.huxingmingcheng}}</view>
      </view>
      <view class="apartment-cen border-box">
          <view class="apartment-cen-font-1">建面</view>
          <view class="apartment-cen-font-2">{{item.jianzumianji}}</view>
      </view>
      <view class="apartment-bot border-box">
        <view>{{item.huxingleixing}}</view>
      </view>
    </view>
  </scroll-view>
</view>


<view class="around-box border-box">
  <view class="common-title border-box">
    <view class="common-title-left">楼盘周边</view>
  </view>
  <view class="map-box detail-margin border-box">
    <map bindcallouttap="tapMap" bindtap="tapMap" class="map" data-active-index="0" enableScroll="{{false}}"
      enableZoom="{{false}}" id="map" latitude="{{latitude}}" longitude="{{longitude}}" markers="{{markers}}" scale="14"
      showLocation="{{false}}"></map>
  </view>
  <view class="around-btn border-box">
    <view class="around-btn">
      <view bindtap="tapMap" class="around-btn-item" data-active-index="0" data-name="交通"
        data-url="/packages_map/pages/map/qqmap/index?name={{name}}&lng={{longitude}}&lat={{latitude}}&activeIndex=0&areaId={{areaId}}">
        <image class="around-item-img" lazyLoad="true"
          src="https://cdn-fhy-file.bgy.com.cn/bu00310-prd1/5fd855fce4b03eca0c208d20"></image>
        <view class="around-item-label">交通</view>
      </view>
      <view bindtap="tapMap" class="around-btn-item" data-active-index="1" data-name="教育"
        data-url="/packages_map/pages/map/qqmap/index?name={{name}}&lng={{longitude}}&lat={{latitude}}&activeIndex=1&areaId={{areaId}}">
        <image class="around-item-img" lazyLoad="true"
          src="https://cdn-fhy-file.bgy.com.cn/bu00310-prd1/5fd855c0e4b03eca0c208d1f"></image>
        <view class="around-item-label">教育</view>
      </view>
      <view bindtap="tapMap" class="around-btn-item" data-active-index="2" data-name="医疗"
        data-url="/packages_map/pages/map/qqmap/index?name={{name}}&lng={{longitude}}&lat={{latitude}}&activeIndex=2&areaId={{areaId}}">
        <image class="around-item-img" lazyLoad="true"
          src="https://cdn-fhy-file.bgy.com.cn/bu00310-prd1/5fd85645e4b03eca0c208d22"></image>
        <view class="around-item-label">医疗</view>
      </view>
      <view bindtap="tapMap" class="around-btn-item" data-active-index="3" data-name="商业"
        data-url="/packages_map/pages/map/qqmap/index?name={{name}}&lng={{longitude}}&lat={{latitude}}&activeIndex=3&areaId={{areaId}}">
        <image class="around-item-img" lazyLoad="true"
          src="https://cdn-fhy-file.bgy.com.cn/bu00310-prd1/5fd85626e4b03eca0c208d21"></image>
        <view class="around-item-label">商业</view>
      </view>
    </view>
  </view>
</view>


<!-- <view class="around-box border-box" wx:if="{{showList.length}}"> -->
<view class="around-box border-box">
  <view class="common-title border-box">
    <view class="common-title-left">楼盘亮点</view>
  </view>
  <view class="swiper-content">
    <scroll-view bindscroll="scrollMove" class="swiper-top" scrollLeft="{{scrollLeft}}" scrollWithAnimation="true"
      scrollX="true">
      <view bindtap="CheckedIcon" class="swiper-top-main {{listIndex==index?'checked':''}}" data-index="{{index}}"
        data-title="{{item.title}}" id="scroll-item-{{index}}" wx:if="{{item.src}}" wx:for="{{showList}}"
        wx:key="index">
        <view class="">
          <view class="swiper-icon {{listIndex==index?'swiper-icon-checked':''}}">
            <image class="iconsrc" data-index="{{index}}" mode="aspectFill" src="{{item.iconsrc}}"></image>
          </view>
          <text class="">{{item.title}}</text>
        </view>
      </view>
    </scroll-view>
    <swiper bindchange="RecommendHouseChange" current="{{listIndex}}" style="height:{{swiperheight}}">
      <swiper-item class="swiper-bottom" wx:for="{{showList}}" wx:key="index">
        <view class="swiper-bottom-main">
          <view class="swiper-item">
            <image binderror="imgError" bindload="bannerload" class="banner" data-index="{{index}}"
              id="swiperimg-{{index}}" mode="widthFix" src="{{item.src}}"></image>
          </view>
        </view>
      </swiper-item>
    </swiper>
  </view>
</view>


<!-- <view class="recommend-house-layer" wx:if="{{isHasData}}"> -->
<view class="recommend-house-layer">
  <view class="head w90">
    <view class="h1">楼盘相册</view>
  </view>
  <view class="swiper-nav-type" wx:if="{{list.length>1}}">
    <view bindtap="RecommendHouseChange" class="swiper-btn {{liseIndex===index?'hover':''}}" data-idx="{{index}}"
      style="{{list.length==2&&'width: 300rpx;'||list.length==1&&'width: 600rpx;'}}" wx:for="{{list}}" wx:key="index">
      {{item.name}}
    </view>
  </view>
  <swiper bindchange="RecommendHouseChange" class="swiper-box" current="{{liseIndex}}" style="{{swiperStyle}}">
    <swiper-item wx:for="{{list}}" wx:key="index">
      <view class="house-list-layer w91">
        <view bind:tap="swiperHandle" class="item {{idx==item.data.length-1&&item.data.length%2!=0&&'lastItem'}}"
          data-imgId="{{itm.id}}" data-type="{{itm.Type}}" data-webUrl="{{itm.link}}" wx:for="{{item.data}}"
          wx:for-index="idx" wx:for-item="itm" wx:key="index">
          <image binderror="imgError" class="banner" data-index="{{idx}}" lazyLoad="true" mode="aspectFix"
            src="{{itm.imgRelUrl}}"></image>
          <CenterBtn ani="{{itm.Type=='2'?'ani':''}}"
            props="width:89rpx;height:89rpx;font-size:28rpx;background:rgba(0, 0, 0, 0.5);"
            src="/resources/{{itm.Type=='1'?'videoplay':itm.Type=='2'?'iconVR':''}}.png"
            title="{{itm.Type=='2'?'VR':''}}" wx:if="{{itm.Type=='1'||itm.Type=='2'}}"></CenterBtn>
        </view>
      </view>
    </swiper-item>
  </swiper>
</view>


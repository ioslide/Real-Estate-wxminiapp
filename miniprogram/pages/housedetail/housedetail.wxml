<swiper class="card-swiper square-dot" indicator-dots="true" circular="true" autoplay="true" interval="5000"
  duration="500" bindchange="cardSwiper" indicator-color="#8799a3" indicator-active-color="#0081ff">
  <swiper-item wx:for="{{houseDetail.loupanxiangce}}" wx:key class="{{cardCur==index?'cur':''}}">
    <view class="swiper-item">
      <image src="{{item}}" mode="aspectFill"></image>
    </view>
  </swiper-item>
</swiper>
<view class="detail-price border-box">
  <view class="detail-title-box detail-margin border-box margin-bottom-20">
    <view class="detail-title-text border-box">{{houseDetail.loupanmingcheng}}</view>
  </view>
  <view class="detail-label-box border-box detail-margin">
    <view class="detail-label-item detail-vr-background" wx:if="{{detail.vrCnt}}">VR看房</view>
    <view class="detail-label-item" wx:for="{{houseDetail.biaoqian}}" wx:key="*this">{{item}}</view>
  </view>
</view>
<view class="floor-detail border-box">

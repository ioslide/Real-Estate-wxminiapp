
page{
  background:#ffffff
}
.common-title {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 84rpx;
  margin-bottom: 26rpx;
}
.cu-card.article>.cu-item .content {
  display: flex;
  padding: 0!important;
}
.anxiang {
  display: -webkit-box;
  display: -moz-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  -moz-line-clamp: 2;
  text-overflow: ellipsis;
  overflow: hidden;
  
  font-size: 16px;
  line-height: 1.5;
  color: #333;
}

.anxiang:before {
  content: "安";
  width: 18px;
  height: 18px;
  text-align: center;
  line-height: 18px;
  color: #fff;
  background-color: #ffb200;
  font-size: 12px;
  border-radius: 2px;
  display: inline-block;
  position: relative;
  top: -1px;
  left: 0;
  margin-right: 4px;
}

.im {
  width: 98rpx;
  height: 98rpx;
  text-align: center;
  position: fixed;
  color: #fff;
  right: 40rpx;
  bottom: 35rpx;
  z-index: 105;
}
.im-readCount {
  width: 100%;
  height: 100%;
  border-radius: 50%;
  position: absolute;
  background-color: rgba(239, 236, 236, 0.8);
  left: 0;
  top: 0;
  font-size: 24rpx;
  line-height: 16rpx;
  box-sizing: border-box;
  padding-top: 14rpx;
  z-index: 104;
}

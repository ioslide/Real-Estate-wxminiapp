.cu-form-group .title {
  min-width: calc(4em + 30rpx);
}
page{
  background: #ffffff;
}
.title{
  font-size:14px!important
}
.footer-fixed {
  position: fixed;
  top: auto;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 500;
  background-color: #fff;
  box-shadow: 0 -2px 2px hsla(0, 0%, 78%, .3);
}

.footer-fixed .items {
  display: block;
  width: 100%;
  padding: 15px;
  font-size: 16px;
  color: #fff;
  line-height: 1.2;
  text-align: center;
  box-sizing: border-box;
  background: linear-gradient(90deg,#d0bf9d,#b1a183)
}
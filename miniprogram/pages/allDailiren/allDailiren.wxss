.search-layer {
  width: 90%;
  margin-left: 5%;
  display: flex;
}
page{
  background:#ffffff
}
.search {
  flex: 3;
}
.goldTag{
  height: 20px;
  margin-left: 0;
  padding: 0 10rpx;
  font-size: 12px;
  font-weight: 500;
  color: #fff;
  line-height: 1.5;
  background: linear-gradient(90deg,#d0bf9d,#b1a183);
  border-radius: 4rpx;
}
.nameTag{
  height: 20px;
  margin-left: 16rpx;
  padding: 0 10rpx;
  font-size: 12px;
  font-weight: 500;
  color: rgb(0, 0, 0);
  line-height: 1.5;
  background: linear-gradient(90deg,#fafafa,#ffffff);
  border-radius: 4rpx;
}
.search .input {
  color: black;
  height: 76rpx;
  width: 100%;
  margin: 12rpx auto;
  background: #fff;
  border-radius: 10rpx;
  padding: 0 15rpx;
  font-size: 32rpx;
  box-sizing: border-box;
  border: 1px solid rgba(229,229,229,1);
  box-shadow: 0 3rpx 15rpx 0 rgba(0,79,180,0.1);
  display: none;
}

.border-box {
  box-sizing: border-box;
}

.detail-margin {
  margin: 0 40rpx;
}

.scroll-box {
  padding-top: 20rpx;
  box-sizing: border-box;
  height: 92vh;
}

.saleman-scroll-box {
  position: relative;
  width: auto;
}

.saleman-item {
  height: 140rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1rpx solid #EFEFEF;
}

.saleman-item:nth-last-child(2) {
  border: none!important;
}

.saleman-item-left {
  display: flex;
  height: 80rpx;
}

.left-img {
  position: relative;
  width: 80rpx;
  height: 80rpx;
  margin-right: 20rpx;
  border-radius: 50%;
}

.saleman-img-box {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  overflow: hidden;
  background: rgb(245,247,250);
}

.comImg {
  position: absolute;
  width: 46rpx;
  height: 53rpx;
  left: 50%;
  transform: translateX(-50%);
  bottom: -22rpx;
}

.comImg image {
  width: 100%;
  height: 100%;
}

.saleman-item-left-img {
  height: 100%;
  width: 100%;
}

.saleman-item-left-label {
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.saleman-item-label-top {
  height: 20px;
  line-height: 20px;
  margin-bottom: 10rpx;
  font-size: 28rpx;
  font-weight: 500;
  color: rgba(25,25,62,1);
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-family: PingFangSC-Medium,PingFang SC;
}

.saleman-item-label-bot {
  height: 30rpx;
  line-height: 30rpx;
  font-size: 22rpx;
  font-weight: 400;
  color: rgba(147,153,165,1);
  font-family: PingFangSC-Medium,PingFang SC;
}

.saleman-item-right {
  flex: 1;
  display: flex;
  height: 74rpx;
  align-items: center;
  justify-content: flex-end;
}

.saleman-round {
  width: 74rpx;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #e7f4f3;
  border-radius: 50%;
}

.saleman-round-1 {
  width: 74rpx;
  height: 100%;
  margin-right: 40rpx;
}

.saleman-round-pro {
  margin-right: 60rpx;
}

.saleman-item-right-img {
  width: 38rpx;
  height: 38rpx;
}

.saleman-round3 {
  margin-left: 5rpx;
}

.saleman-round-3 {
  width: 74rpx;
  height: 74rpx;
  margin-top: -14rpx;
}

.medalregion-item-right-img {
  width: 100%;
  height: 100%;
}

.saleman-item-right-img {
  width: 38rpx;
  height: 38rpx;
}

.content-box {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
  background-color: #fff;
}